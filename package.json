{
	"name": "CosmosDB-with-Mongo-API",
	"version": "0.1.20",
	"versionDate": "2021-02-13",
	"author": "hackolade",
	"engines": {
		"hackolade": "1.10.x",
		"hackoladePlugin": "1.0.0"
	},
	"contributes": {
		"target": {
			"applicationTarget": "COSMOSDB-MONGO",
			"title": "Cosmos DB w/ Mongo API",
			"versions": [
			]
		},
		"features": {
			"nestedCollections": false,
			"forwardEngineering": {
				"jsonSchema": {
					"keepParentType": {
						"objectId": {
							"type": "string",
							"pattern": "^[a-fA-F0-9]{24}$"
						},
						"regex": "string",
						"date": {
							"type": "string",
							"format": "date-time"
						},
						"timestamp": {
							"type": "string",
							"format": "date-time"
						},
						"binary": {
							"type": "string",
							"mediaType": "image/png",
							"contentEncoding": "base64"
						},
						"JavaScript": "string",
						"JavaScript(w/scope)": "string",
						"symbol": "string",
						"minKey": "number",
						"maxKey": "number"
					}
				}
			},
			"enableReverseEngineering": true,
			"enableReverseEngineeringDetectionSchemaByContent": true,
			"enableReversingDateTypeAsISODate": true
		}
	},
	"description": "Hackolade plugin for Azure Cosmos DB with MongoDB API"
}
